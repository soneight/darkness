	"tokenColors": [
		{
			"name": "Grey",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": { "foreground": "#808080" }
		},
		{
			"name": "White",
			"scope": [
				"constant",
				"keyword.operator",
				"string.quoted"
			],
			"settings": { "foreground": "#C0C0C0" }
		},
		{
			"name": "Yellow",
			"scope": [
				"entity.name.function",
				"support.function"
			],
			"settings": { "foreground": "#C0C080" }
		},
		{
			"name": "Blue",
			"scope": [
				"storage.modifier",
			],
			"settings": { "foreground": "#8080C0" }
		},
		{
			"name": "Green",
			"scope": [
				"entity.name.type",
				"storage.type",
				"support.type"
			],
			"settings": { "foreground": "#80C080" }
		},
		{
			"name": "Cyan",
			"scope": [
				"constant.other.placeholder",
				"variable",
				"entity.other.attribute-name"
			],
			"settings": { "foreground": "#80C0C0" }
		},
		{
			"name": "Magenta",
			"scope": [
				"constant.character.escape",
				"keyword"
			],
			"settings": { "foreground": "#C080C0" }
		},
		{
			"name": "Red",
			"scope": [
				"keyword.control",
				"punctuation",
				"entity.name.tag"
			],
			"settings": { "foreground": "#C08080" }
		},
		{
			"name": "Italic",
			"scope": [
				"constant",
				"entity.name.function.preprocessor",
				"string.quoted"
			],
			"settings": { "fontStyle": "italic" }
		},
		{
			"name": "Bold",
			"scope": [
				"keyword",
				"punctuation",
				"entity.name.scope-resolution"
			],
			"settings": { "fontStyle": "bold" }
		},
		{
			"name": "Strikethrough",
			"scope": [
				"punctuation.separator.pointer-access"
			],
			"settings": { "fontStyle": "bold strikethrough" }
		}
	]
}
